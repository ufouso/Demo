package com.test.java.override_overload;
/**
 * 多个构造方法
 * @Description 
 * @author xpp
 * @date 2017年9月5日 下午5:22:12
 */
public class Test11 {
	
	private String name;
	
	private String age;

	public Test11(){
		System.out.println("第一个构造函数");
	}
	public Test11(String name){
		System.out.println(name);
	}
	
	public Test11(String name, String age){
		System.out.println(name+"-----"+age);
	}

	public static void main(String[] args) {
		Test11 s = new Test11();
		Test11 s1 = new Test11("faker");
		Test11 s2 = new Test11("faker","18");
	}
}
